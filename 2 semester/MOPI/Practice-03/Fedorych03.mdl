"@a0" : "_" -> "@as|.": "|" -> "@a1|<";
"@a1" : "_" -> "@a2z>";
"@a2" : "_" -> "@a6_<": "|" -> "@a3c<": "x" -> "@a2z<": "c" -> "@a2c>";
"@a3" : "_" -> "@a4_>": "z" -> "@a3z<": "x" -> "@a3z<": "c" -> "@a3c<";
"@a4" : "z" -> "@a5x<": "x" -> "@a4x>": "c" -> "@a2c>";
"@a5" : "_" -> "@a4x>": "x" -> "@a5x<";
"@a6" : "z" -> "@a6z<": "x" -> "@a8x>": "c" -> "@a7q>": "q" -> "@a6q<";
"@a7" : "_" -> "@a6z<": "z" -> "@a7z>": "q" -> "@a7q>";
"@a8" : "_" -> "@a9_<": "z" -> "@a8z>": "q" -> "@a8q>";
"@a9" : "_" -> "@ab_>": "z" -> "@aax>": "x" -> "@a9x<": "c" -> "@a9c<": "q" -> "@a9q<";
"@aa" : "_" -> "@a9c<": "x" -> "@aax>": "c" -> "@aac>";
"@ab" : "_" -> "@as_<": "z" -> "@ab|>": "x" -> "@ab|>": "c" -> "@ab|>": "q" -> "@ab|>";
end expr3

