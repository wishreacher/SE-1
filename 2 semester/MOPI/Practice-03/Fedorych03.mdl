'expr1(x, y) = y + 3
Machine expr1
 Alphabet "|#", ""; Numerical 2;
 Initial  "@a0";  Final "@as";
"@a0" : "|" -> "@a0_>": "#" -> "@a1_>"; 'removing the x
"@a1" : "_" -> "@a2|>": "|" -> "@a1|>"; 'adding 1
"@a2" : "_" -> "@a3|>": "|" -> "@a2|>"; 'adding 1
"@a3" : "_" -> "@as|.": "|" -> "@a3|>"; 'adding 1
end expr1

'expr2(x, y, z) = z - (x + y)
Machine expr2
 Alphabet "|#", ""; Numerical 3;
 Initial  "@a0";  Final "@as";
"@a0" : "|" -> "@a0|>": "#" -> "@a1|."; '---
"@a1" : "_" -> "@a2_>": "|" -> "@a1|<"; '---
"@a2" : "|" -> "@a3_>"; 'закінчили додавання
"@a3" : "|" -> "@a4_>": "#" -> "@as_>"; 'почали віднімання, тут є вихід
"@a4" : "_" -> "@a5_<": "|" -> "@a4|>": "#" -> "@a4#>"; 'останній елемент
"@a5" : "|" -> "@a6_<"; 'прибрали ласт палку
"@a6" : "_" -> "@a3_>": "|" -> "@a6|<": "#" -> "@a6#<"; 'повернулись в початок і залупились
end expr2

'expr3(x) = 2^x + 3*x
