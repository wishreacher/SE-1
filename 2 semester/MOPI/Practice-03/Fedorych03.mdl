'less(x, y) 1 if x < y, 0 if x >= y
Machine less
 Alphabet "|#", ""; Numerical 2;
 Initial  "@a0";  Final "@as";
"@a0" : "_" -> "@a1_<": "|" -> "@a0|>": "#" -> "@a0#>"; 'ідемо в кінець і міняємо стан коли дійшли
"@a1" : "|" -> "@a2_<": "#" -> "@a8#."; 'відняли одну крайню палку
"@a2" : "_" -> "@a3_>": "|" -> "@a2|<": "#" -> "@a2#<"; 'прийшли на початок
"@a3" : "|" -> "@a4_>"; 'відняли палку на початку
"@a4" : "_" -> "@a5_<": "|" -> "@a4|>": "#" -> "@a4#>"; 'прийшли в кінець
"@a5" : "|" -> "@a6|.": "#" -> "@a8#."; 'якщо дієз то ігрик менше, повернути нуль!!!!!!!! якщо ні то йдемо далі 
"@a6" : "_" -> "@a7_>": "|" -> "@a6|<": "#" -> "@a6#<"; 'прийшли в початок 
"@a7" : "|" -> "@a0|.": "#" -> "@a9#."; 'якшо дієз то повернути 1, якщо ні залупити!!!!!!!
"@a8" : "_" -> "@as_.": "|" -> "@a8_<": "#" -> "@a8_<"; 'все прибрати зліва направо і в кінці написати нуль
"@a9" : "_" -> "@as|.": "|" -> "@a9_>": "#" -> "@a9_>"; 'все прибрати справа наліво і написати одиницю
end less

'new
Machine remain
 Alphabet "|#", "ZXC"; Numerical 2;
 Initial  "@a0";  Final "@as";
"@a0" : "_" -> "@a1Z>": "|" -> "@a0|<"; 'поставили Z на початку
"@a1" : "_" -> "@a2Z<": "|" -> "@a1|>": "#" -> "@a1#>"; 'поставили Z в кінці
"@a2" : "|" -> "@a3C<"; 'поміняли крайню праву паличку на C
"@a3" : "|" -> "@a3|<": "#" -> "@a4#<"; 'прийшли на перший елемент зліва від дієза
"@a4" : "|" -> "@a5X>": "#" -> "@a4#<": "Z" -> "@a9_>": "X" -> "@a4X<"; 'Z!!! угабугааааа
"@a5" : "|" -> "@a5|>": "#" -> "@a5#>": "X" -> "@a5X>": "C" -> "@a6C<"; 'повернутись до останньої палки
"@a6" : "|" -> "@a2|.": "#" -> "@a7#."; 'якщо палка - залупити, якщо 0 перезапустити з початку !!!!!!!!!!!
"@a7" : "|" -> "@a7|<": "#" -> "@a7#<": "Z" -> "@a8Z>": "X" -> "@a7X<": "C" -> "@a7C<";
"@a8" : "|" -> "@a8|>": "#" -> "@a8#>": "Z" -> "@a2Z<": "X" -> "@a8#>": "C" -> "@a8|>"; '!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
"@a9" : "|" -> "@a9|>": "#" -> "@a9#>": "Z" -> "@aa_<": "X" -> "@a9X>": "C" -> "@a9C>"; 'тут треба повернутися в кінець
"@aa" : "_" -> "@as_>": "|" -> "@aa_<": "#" -> "@aa_<": "Z" -> "@as_.": "X" -> "@aa|<": "C" -> "@aa_<"; 'прибирання
end remain
